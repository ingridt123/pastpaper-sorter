<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="address.view.TeacherDialogueController">
   <children>
      <TextField fx:id="codeEnter" layoutX="146.0" layoutY="31.0" prefHeight="26.0" prefWidth="136.0" AnchorPane.topAnchor="31.0" />
      <Button fx:id="student" layoutX="26.0" layoutY="100.0" mnemonicParsing="false" onAction="#handleStudent" prefWidth="110.0" text="I am a student." AnchorPane.bottomAnchor="24.0" />
      <Button fx:id="teacher" defaultButton="true" layoutX="160.0" layoutY="100.0" mnemonicParsing="false" onAction="#handleTeacher" text="I am a teacher." AnchorPane.bottomAnchor="24.0" />
      <Label layoutX="19.0" layoutY="36.0" style="-fx-background-color: #f3f3f3;" text="Enter teacher code:" AnchorPane.topAnchor="36.0">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets left="3.0" right="3.0" />
         </padding>
      </Label>
      <Label fx:id="errorMsg" layoutX="26.0" layoutY="74.0" textFill="RED">
         <font>
            <Font size="11.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
